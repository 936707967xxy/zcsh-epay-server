<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
  
<mapper namespace="com.zcsh.epay.modules.user.mapper.LoginUserMapper">

<select id="queryMerchantList" parameterType="com.zcsh.epay.modules.user.vo.req.UserInfoReq"
        resultType="com.zcsh.epay.modules.user.vo.req.UserInfoReq">
		SELECT 
		bal.LOAN_ID AS userId,
		bal.CON_NO AS userName
		FROM AFTER_LOANBAL bal
</select>
</mapper>  